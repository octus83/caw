<Window x:Class="bgx_caw_backend.ImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Neuer CAW-Plan" Height="164.95" ShowInTaskbar="False" WindowStartupLocation="Manual" ResizeMode="NoResize" Width="287"  >
    <StackPanel HorizontalAlignment="Left" Height="146" VerticalAlignment="Top" Width="274" Margin="0,0,0,-4">
        <Label Content="DXF-Ordner auswählen"/>
        <StackPanel Height="25" Orientation="Horizontal">
            <TextBox x:Name="tbox_sourceFolder" TextWrapping="Wrap" Text="TextBox" Width="190"/>
            <Button x:Name="btn_chooseFolder" Content="Button" Margin="0,0,449,0" Width="69" Click="btn_chooseFolder_Click"/>
        </StackPanel>
        <Button x:Name="btn_import" Content="Button" Margin="15,50,26,0" Click="btn_import_Click"/>
    </StackPanel>

</Window>
